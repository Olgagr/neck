{
  "name": "neck",
  "version": "0.1.1",
  "authors": [
    "smalluban"
  ],
  "description": "Create apps with Backbone faster!",
  "main": "lib/neck.js",
  "keywords": [
    "backbone",
    "backbonejs",
    "plugin",
    "neck",
    "neckjs",
    "mvc",
    "binding",
    "data-binding",
    "nested-views"
  ],
  "license": "MIT",
  "homepage": "neckjs.org",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "underscore": ">=1.5.0",
    "backbone": "~1.1.0",
    "backbone-query-parameters": "~0.3.0"
  },
  "overrides": {
    "backbone": {
      "main": "backbone.js"
    },
    "backbone-query-parameters": {
      "main": [
        "backbone.queryparams.js",
        "backbone.queryparams.js-1.1-shim.js"
      ]
    },
    "underscore": {
      "main": "underscore.js"
    }
  }
}
